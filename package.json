{
  "name": "react-ts-seed",
  "version": "0.1.0",
  "private": true,
  "pre-commit": [
    "lint",
    "prettier:check"
  ],
  "scripts": {
    "start": "yarn start:local",
    "start:local": "dotenv -e ./src/config/.env.local react-scripts start",
    "start:dev": "dotenv -e ./src/config/.env.development react-scripts start",
    "start:qa": "dotenv -e ./src/config/.env.qa react-scripts start",
    "build": "yarn build:production",
    "build:development": "dotenv -e ./src/config/.env.development react-scripts build",
    "build:qa": "dotenv -e ./src/config/.env.qa react-scripts build",
    "build:staging": "dotenv -e ./src/config/.env.staging react-scripts build",
    "build:production": "dotenv -e ./src/config/.env.production react-scripts build",
    "test": "dotenv -e ./src/config/.env.test cross-env TZ=America/Los_Angeles react-scripts test  -- --watchAll=false --env=jsdom --coverage --forceExit --detectOpenHandles --verbose --testResultsProcessor jest-sonar-reporter",
    "test:watch": "dotenv -e ./src/config/.env.test cross-env TZ=America/Los_Angeles react-scripts test  -- --watchAll=true --env=jsdom --coverage --forceExit --detectOpenHandles --verbose --testResultsProcessor jest-sonar-reporter --updateSnapshot",
    "test-ci": "yarn test --reporters=jest-junit",
    "test:first": "yarn test -u --bail",
    "lint": "tslint -c ./tslint.json 'src/**/*.{ts,tsx}'",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer build/static/js/main.* --html > tree.html && google-chrome tree.html",
    "prettier": "prettier --parser typescript --single-quote",
    "prettier:check": "yarn prettier --list-different \"src/**/*.{ts,tsx}\"",
    "format": "yarn prettier --write \"src/**/*.{ts,tsx}\""
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/registerServiceWorker.ts",
      "!src/modules/models/**/*.*",
      "!src/**/*.d.ts",
      "!src/**/styles.ts",
      "!src/modules/state-mgmt/rootState.ts",
      "!src/modules/state-mgmt/store.ts",
      "!src/modules/state-mgmt/EpicRegistry.ts",
      "!src/modules/state-mgmt/ReducerRegistry.ts",
      "!src/modules/AppRoot.tsx",
      "!src/test/**/*.*",
      "!src/**/index.{ts,tsx}",
      "!src/**/FormData.ts",
      "!src/constants/**/*.*"
    ],
    "coverageReporters": [
      "lcov",
      "text",
      "text-summary"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    }
  },
  "dependencies": {
    "@aws-amplify/auth": "^3.4.17",
    "@date-io/moment": "^1.3.13",
    "@material-ui/core": "^4.9.13",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.58",
    "@material-ui/pickers": "^3.2.10",
    "@stripe/react-stripe-js": "^1.1.2",
    "@stripe/stripe-js": "^1.11.0",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/material-ui": "^0.21.8",
    "@types/react-datepicker": "^4.4.1",
    "autosuggest-highlight": "^3.1.1",
    "connected-react-router": "^6.8.0",
    "cross-env": "^6.0.3",
    "libphonenumber-js": "^1.7.53",
    "moment": "^2.25.3",
    "moment-timezone": "^0.5.33",
    "mutationobserver-shim": "^0.3.5",
    "pre-commit": "^1.2.2",
    "react": "^16.13.1",
    "react-datepicker": "^4.7.0",
    "react-dom": "^16.13.1",
    "react-gtm-module": "^2.0.11",
    "react-loading-indicator": "^1.0.2",
    "react-phone-input-2": "^2.14.0",
    "react-player": "^2.9.0",
    "react-redux": "^7.2.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.1",
    "react-test-renderer": "^16.13.1",
    "react-text-mask": "^5.4.3",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-observable": "^1.2.0",
    "rxjs": "^6.6.3",
    "start-server-and-test": "^1.11.0",
    "text-mask-addons": "^3.8.0",
    "typeface-hind": "0.0.72",
    "victory": "^35.4.3"
  },
  "devDependencies": {
    "@testing-library/react": "^9.5.0",
    "@types/history": "^4.7.5",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.37",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^16.9.6",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.4",
    "@types/react-test-renderer": "^16.9.2",
    "@types/redux": "^3.6.0",
    "@types/redux-devtools-extension": "^2.13.2",
    "dotenv-cli": "^3.1.0",
    "jest-junit": "^11.1.0",
    "jest-sonar-reporter": "^2.0.0",
    "prettier": "^1.19.1",
    "react-docgen-typescript-loader": "^3.7.2",
    "redux-test-utils": "^1.0.0",
    "source-map-explorer": "^2.4.2",
    "tslint": "^5.20.1",
    "tslint-react": "^4.1.0",
    "typescript": "^3.8.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "printWidth": 160,
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "quoteProps": "as-needed",
    "jsxSingleQuote": false,
    "jsxBracketSameLine": false,
    "proseWrap": "preserve"
  },
  "jest-junit": {
    "outputDirectory": "./test-results/junit",
    "outputName": "results.xml"
  }
}
